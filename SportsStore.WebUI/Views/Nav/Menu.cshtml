@model IList<string>

@Html.ActionLink("Home", "List", "Product")

@for (var i = 0; i < Model.Count(); i++)
{
    @Html.RouteLink(Model[i], 
                    new { controller = "Product", action = "List", categoryOn = ViewBag.CategoryOn, category = i, page = 1 },
                    new { @class = ViewBag.CategoryOn == true && i == ViewBag.SelectedCategory ? "selected" : null} )
}